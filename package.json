{
  "name": "adversarial-search-js",
  "version": "0.1.0",
  "description": "A NPM package that implements adversarial search algoritms.",
  "main": "build/index.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "test": "nyc mocha --require babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilianoSanchez/adversarial-search-js.git"
  },
  "keywords": [
    "npm",
    "node",
    "javascript",
    "adversarial search"
  ],
  "author": "Luis Emiliano Sanchez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmilianoSanchez/adversarial-search-js/issues"
  },
  "homepage": "https://github.com/EmilianoSanchez/adversarial-search-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "husky": "^1.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "lodash": "^4.17.13"
  }
}
